page{
  background: #eee;
}
.search-header{
  width: 100%;
  padding-top: 10rpx;
  height:100rpx;
  background: #03bbd5;
  display: flex;
  justify-content: space-between;
  padding-left: 20rpx;
  position: fixed;
  top: 0;
}
.search-input{
  display: inline-block;
  background: #fff;
  width: 100%;
  height: 70rpx;
  line-height: 90rpx;
  font-size: 30rpx;
  margin: auto;
  border-radius: 10rpx;
  padding: 0 10rpx;
}
.search-btn{
  margin-right: 10rpx;
  color: #fff;
  width:160rpx;
  line-height: 100rpx;
  text-align: center;
  font-size: 30rpx;
}
.item{
  background: #fff;
  margin-bottom: 20rpx;
  padding: 25rpx;
  font-size: 30rpx;
}
.item-address{
  font-size: 27rpx;
  color:#ff9700;
  margin-bottom: 10rpx;
}
.item-message{
  color:#777;
}